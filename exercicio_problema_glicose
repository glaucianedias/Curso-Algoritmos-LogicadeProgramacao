Algoritmo "problema_glicose"

Var

   medida : real

Inicio

   escreva("Digite a medida da glicose: ")
   leia(medida)
   
   se (medida > 0) e (medida <= 100) entao
      escreva("Classificação: Normal.")
   senao
      se (medida > 101) e (medida <= 140) entao
         escreva("Classificação: Elevado.")
      senao
         se (medida = 141) ou (medida > 141) entao
            escreva("Classificação: Diabetes.")
         fimse
      fimse
   fimse

Fimalgoritmo
